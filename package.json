{
  "name": "vue3-antd-admin",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "serve": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "build typecheck": "vuedx-typecheck . && vite build",
    "deploy": "gh-pages -d dist",
    "lint:eslint": "eslint \"{src,mock}/**/*.{vue,ts,tsx}\" --fix",
    "lint:prettier": "prettier --write --loglevel warn \"src/**/*.{js,json,tsx,css,less,scss,vue,html,md}\"",
    "lint:stylelint": "stylelint --fix \"**/*.{vue,less,postcss,css,scss}\" --cache --cache-location node_modules/.cache/stylelint/",
    "lint:lint-staged": "lint-staged -c ./.husky/lintstagedrc.js",
    "lint:pretty": "pretty-quick --staged",
    "test prod gzip": "http-server dist --cors --gzip -c-1",
    "test prod cors": "http-server dist --cors --gzip -P http://us-la-cn2.sakurafrp.com:59660"
  },
  "dependencies": {
    "@vueuse/core": "^4.8.1",
    "ant-design-vue": "^2.1.2",
    "axios": "^0.21.1",
    "blueimp-md5": "^2.18.0",
    "dayjs": "^1.8.35",
    "lodash": "^4.17.20",
    "mitt": "^2.1.0",
    "nprogress": "^1.0.0-1",
    "qs": "^5.2.1",
    "vue": "^3.0.11",
    "vue-router": "^4.0.8",
    "vuex": "^4.0.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^12.1.4",
    "@commitlint/config-conventional": "^12.1.4",
    "@types/lodash": "^4.14.170",
    "@types/node": "^14.17.1",
    "@typescript-eslint/eslint-plugin": "^4.25.0",
    "@typescript-eslint/parser": "^4.25.0",
    "@vitejs/plugin-vue": "^1.2.2",
    "@vitejs/plugin-vue-jsx": "^1.1.4",
    "@vue/compiler-sfc": "^3.0.11",
    "@vue/eslint-config-typescript": "^5.1.0",
    "@vuedx/typecheck": "^0.6.3",
    "@vuedx/typescript-plugin-vue": "^0.6.3",
    "commitizen": "^4.2.4",
    "core-js": "^3.10.1",
    "eslint": "^7.27.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^3.4.0",
    "eslint-plugin-vue": "^7.2.0",
    "gh-pages": "^3.1.0",
    "husky": "^6.0.0",
    "less": "^3.12.2",
    "less-loader": "^5.0.0",
    "lint-staged": "^11.0.0",
    "prettier": "^2.3.0",
    "pretty-quick": "^3.1.0",
    "sass": "^1.34.0",
    "sass-loader": "^10.0.2",
    "stylelint": "^13.13.1",
    "stylelint-config-prettier": "^8.0.2",
    "stylelint-config-standard": "^22.0.0",
    "stylelint-order": "^4.1.0",
    "typescript": "^4.2.4",
    "vite": "^2.3.4",
    "vite-plugin-style-import": "^0.10.1",
    "vue-eslint-parser": "^7.6.0"
  },
  "lint-staged": {
    "*.{vue,js,ts,tsx}": "eslint --fix"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-customizable"
    }
  },
  "keywords": [
    "vue",
    "ant-design-vue",
    "vue3",
    "ts",
    "tsx",
    "admin",
    "typescript"
  ],
  "homepage": "git@buqiyuan.github.io/vue3-antd",
  "__npminstall_done": false
}
